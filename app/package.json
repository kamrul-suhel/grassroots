{
    "name": "xanda",
    "version": "1.0.0",
    "scripts": {
        "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress --profile --colors",
        "build:css": "node-sass --include-path scss sass/layout.scss build/css/layout.css",
        "build": "npm run copy-files && npm run build:css && npm run build:webpack --progress --profile --colors",
        "copy-files": "mkdirp build && cp index.html build/ && cp -R images build/ && cp -R fonts build/",
        "dev": "cross-env NODE_ENV=development npm start",
        "kill-win-node": "taskkill /f /im node.exe",
        "start": "node ./js/server.js"
    },
    "license": "MIT",
    "dependencies": {
        "@xanda/react-components": "^1.0.302",
        "axios": "^0.18.0",
        "create-react-class": "^15.6.3",
        "draft-js": "^0.10.5",
        "draft-js-export-html": "^1.3.3",
        "draftjs-to-html": "^0.8.4",
        "express": "^4.16.2",
        "html-to-draftjs": "^1.4.0",
        "lodash": "^4.17.11",
        "moment": "^2.20.1",
        "prop-types": "^15.6.0",
        "react": "^16.5.2",
        "react-big-calendar": "^0.20.1",
        "react-dom": "^16.2.0",
        "react-draft-wysiwyg": "^1.12.13",
        "react-hot-loader": "^3.1.3",
        "react-redux": "^5.0.7",
        "react-router": "^3.0.2",
        "react-router-redux": "^4.0.8",
        "react-tabs": "^2.2.1",
        "redux": "^3.7.2",
        "redux-devtools": "^3.4.1",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.2.0",
        "reselect": "^3.0.1",
        "socket.io": "^2.0.4",
        "universal-cookie": "^2.1.2"
    },
    "devDependencies": {
        "autoprefixer": "^8.0.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.3",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-2": "^6.24.1",
        "cross-env": "^5.1.3",
        "css-loader": "^0.28.10",
        "file-loader": "^1.1.9",
        "mkdirp": "^0.5.1",
        "node-sass": "^4.7.2",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.20.2",
        "url-loader": "^0.6.2",
        "webpack": "^4.0.0",
        "webpack-bundle-analyzer": "^2.11.0",
        "webpack-cli": "^2.0.9",
        "webpack-dev-middleware": "^2.0.6",
        "webpack-hot-middleware": "^2.21.0"
    }
}
